{
  "name": "dato-konverter",
  "version": "2.0.0",
  "type": "module",
  "description": "Konverter JavaScript-datoer til dansk format med korrekt tidszone. Understøtter formater som '15. okt. 2024 kl. 13:00' og tilbyder funktioner som dansk ugedag, ugenummer og weekend-detektion.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
  "dist/index.js",
  "dist/index.d.ts",
  "README.md"
],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "start": "node dist/examples.js",
    "dev": "vitest --watch"
  },
  "author": "Chrilleweb",
  "license": "MIT",
  "keywords": [
    "dato",
    "konverter",
    "dansk",
    "date",
    "timezone",
    "format",
    "danish",
    "weeknumber",
    "date-utils",
    "ugedag",
    "ugenummer",
    "klokkeslæt",
    "tid",
    "weekend",
    "datetime"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Chrilleweb/dato-konverter"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vitest": "^3.2.0"
  }
}
